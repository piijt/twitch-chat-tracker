{
  "name": "twitch-chat-tracker",
  "version": "1.0.0",
  "description": "Tracking twitch chat, and can be used to calculate viewer retention based on viewers chat activity",
  "main": "index.js",
  "scripts": {
    "start-tw-ts": "ts-node src/chat_tracker.ts",
    "start-tw": "node js/index.js",
    "build:client": "tsc",
    "start": "npm run serve",
    "dev": "npm run watch",
    "build": "npm run build-ts",
    "serve": "node dist/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/src/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint src/*.ts",
    "lint-fix": "yarn lint --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "Peter H. Jespersen",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "sentiment": "^5.0.2",
    "tmi.js": "^1.8.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/tmi.js": "^1.8.1",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0"
  }
}
